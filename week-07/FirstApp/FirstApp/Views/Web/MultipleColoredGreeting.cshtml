@model FirstApp.Models.MultipleColoredGreeting
<!DOCTYPE HTML>
<html xmlns:th="http://www.thymeleaf.org">
<head>
	<title>Getting Started: Serving Web Content</title>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>
<body>
	@foreach (var item in @Model.hellos)
	{
		<span style="font-size: @Model.GetFontSize()px; color: rgb(@Model.GetColor(), @Model.GetColor(), @Model.GetColor())">
			@item
		</span>
	}
</body>
</html>